{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "webkompanen-blocks/reviews",
	"title": "Reviews",
	"category": "webkompanen-blocks/querys",
	"description": "Reviews",
	"textdomain": "webkompanen",
	"attributes": {
		"align":{
			"type": "string",
			"default": "full"
		},
		"content":{
			"type": "string"
		},
        "classes":{
            "type": "string"
        },
		"post":{
			"type": "object"
		},
		"image":{
			"type": "object"
		},
		"imageId":{
			"type": "number"
		},
		"imageUrl":{
			"type": "string"
		},
		"imageAlt":{
			"type": "string"
		},
		"number_of_items":{
			"type": "number",
			"default": -1
		},
		"title":{
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"align": [ "wide", "full" ]
	}
}
