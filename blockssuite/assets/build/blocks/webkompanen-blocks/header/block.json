{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "webkompanen/header",
  "title": "Header",
  "category": "webkompanen/layout",
  "description": "Display header content",
  "textdomain": "webkompanen",
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "content": {
      "type": "string"
    },
    "post": {
      "type": "object"
    },
    "headerImage": {
      "type": "object"
    },
    "headerImageId": {
      "type": "number"
    },
    "headerImageUrl": {
      "type": "string"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  }
}