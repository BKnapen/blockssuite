{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "webkompanen-blocks/eventinfo",
  "title": "Eventinfo",
  "category": "webkompanen-blocks/querys",
  "description": "Return a list of last posts from the database",
  "textdomain": "webkompanen",
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "content": {
      "type": "string"
    },
    "classes": {
      "type": "string"
    },
    "post": {
      "type": "object"
    },
    "image": {
      "type": "object"
    },
    "imageId": {
      "type": "number"
    },
    "imageUrl": {
      "type": "string"
    },
    "imageAlt": {
      "type": "string"
    },
    "number_of_items": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "wrapper": {
      "type": "boolean"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  }
}