{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "webkompanen-blocks/lastposts",
  "title": "Last posts",
  "category": "webkompanen-blocks/querys",
  "description": "Return a list of last posts from the database",
  "textdomain": "webkompanen",
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    },
    "content": {
      "type": "string"
    },
    "classes": {
      "type": "string"
    },
    "post": {
      "type": "object"
    },
    "image": {
      "type": "object"
    },
    "imageId": {
      "type": "number"
    },
    "imageUrl": {
      "type": "string"
    },
    "imageAlt": {
      "type": "string"
    },
    "number_of_items": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "wrapper": {
      "type": "boolean"
    },
    "post_type": {
      "type": "boolean"
    },
    "tag_type": {
      "type": "boolean"
    },
    "tags": {
      "type": "boolean"
    },
    "showaslist": {
      "default": true,
      "type": "boolean"
    },
    "showpostthumbnail": {
      "default": true,
      "type": "boolean"
    },
    "colxs": {
      "type": "number"
    },
    "colautoxs": {
      "type": "boolean"
    },
    "colsm": {
      "type": "number"
    },
    "colautosm": {
      "type": "boolean"
    },
    "colmd": {
      "type": "number"
    },
    "colautomd": {
      "type": "boolean"
    },
    "collg": {
      "type": "number"
    },
    "colautolg": {
      "type": "boolean"
    },
    "colxl": {
      "type": "number"
    },
    "colautoxl": {
      "type": "boolean"
    },
    "colxxl": {
      "type": "number"
    },
    "colautoxxl": {
      "type": "boolean"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  }
}